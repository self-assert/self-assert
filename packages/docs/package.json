{
  "name": "@self-assert/docs",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "npm run docs:api && npm run docs:build",
    "dev": "vitepress dev site",
    "docs:api": "npm run docs:api-core && npm run docs:api-react",
    "docs:api-core": "typedoc --options typedoc.core.json",
    "docs:api-react": "typedoc --options typedoc.react.json",
    "docs:build": "vitepress build site",
    "preview": "vitepress preview site"
  },
  "license": "MIT",
  "devDependencies": {
    "typedoc": "^0.28.13",
    "typedoc-plugin-frontmatter": "^1.3.0",
    "typedoc-plugin-markdown": "^4.9.0",
    "typedoc-vitepress-theme": "^1.1.2",
    "vitepress": "^1.6.4",
    "vitepress-plugin-group-icons": "^1.6.3"
  },
  "type": "module"
}
